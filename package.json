{
  "name": "cucumber-boilerplate",
  "version": "1.1.0",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "main": "test/runner.js",
  "scripts": {
    "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" -n 1 -r; if [[ $REPLY =~ ^[Yy]$ ]]; then sudo rm -R .git .github demo-app test .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
    "local-webserver": "http-server ./demo-app -s",
    "test": "npm run test:validate && npm run test:unit && npm run test:features",
    "test:features": "npm run-script local-webserver & (wdio wdio.BUILD.conf.js; wdio_ret=$?; kill $(lsof -t -i:8080); exit $wdio_ret)",
    "test:unit": "jest --config=jest.json",
    "test:validate": "eslint 'src/**/*.js' 'test/**/*.js!(setup.js)'",
    "login": "node_modules\\.bin\\wdio wdio.conf.js --specs login.feature & node report_processor.js",
    "logout": "node_modules\\.bin\\wdio wdio.conf.js --specs logout.feature & node report_processor.js",
    "licenseRegistrationElements": "node_modules\\.bin\\wdio wdio.conf.js --specs Elementi//licenseRegistrationElements.feature & node report_processor.js",
    "licenseRegistrationInvalid": "node_modules\\.bin\\wdio wdio.conf.js --specs Elementi//licenseRegistrationInvalid.feature & node report_processor.js",
    "licenseRegistrationValid": "node_modules\\.bin\\wdio wdio.conf.js --specs Elementi//licenseRegistrationValid.feature & node report_processor.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/cucumber-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/cucumber-boilerplate/issues"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "test",
    "selenium"
  ],
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "~6.18.0",
    "babel-register": "~6.18.0",
    "chai": "~3.5.0",
    "cucumber": "^1.3.1",
    "wdio-cucumber-framework": "~0.2.15",
    "wdio-phantomjs-service": "~0.2.2",
    "wdio-selenium-standalone-service": "~0.0.7",
    "wdio-spec-reporter": "~0.0.5",
    "webdriverio": "~4.6.1"
  },
  "devDependencies": {
    "babel-jest": "~18.0.0",
    "babel-polyfill": "~6.20.0",
    "eslint": "~3.13.1",
    "eslint-config-airbnb-base": "~11.0.1",
    "http-server": "~0.9.0",
    "jest": "~18.1.0",
    "junit-viewer": "^4.11.0",
    "wdio-junit-reporter": "^0.2.0"
  }
}
